{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "preview": "yarn build && yarn start",
    "lint": "next lint",
    "postbuild": "next-sitemap --config next-sitemap.config.mjs",
    "lint:styles": "stylelint \"src/**/*.{css,scss}\"",
    "fix:styles": "stylelint --fix \"src/**/*.{css,scss}\"",
    "lint:scripts": "eslint --ext .ts,.tsx . --max-warnings=0",
    "format": "prettier --write . --ignore-unknown",
    "commitmsg": "commitlint -e $GIT_PARAMS"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn run type-check"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": "yarn lint:scripts",
    "*.{css,scss}": "yarn lint:styles",
    "*": "yarn format"
  },
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@fontsource/inter": "^4.5.15",
    "@fontsource/noto-sans-jp": "^4.5.12",
    "@mui/icons-material": "^5.11.11",
    "@mui/material": "^5.11.11",
    "@nanostores/react": "^0.4.1",
    "@types/node": "18.14.6",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "auto-bind": "^5.0.1",
    "clsx": "^1.2.1",
    "dayjs": "^1.11.7",
    "gray-matter": "^4.0.3",
    "is-touch-device": "^1.0.1",
    "microcms-js-sdk": "^2.3.2",
    "nanostores": "^0.7.4",
    "next": "13.2.3",
    "next-auth": "^4.20.1",
    "next-seo": "^5.15.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remark": "^14.0.2",
    "remark-breaks": "^3.0.2",
    "remark-external-links": "^9.0.1",
    "remark-gfm": "^3.0.1",
    "remark-html": "^15.0.2",
    "remark-slug": "^7.0.1",
    "remove-markdown": "^0.5.0",
    "ress": "^5.0.2",
    "typescript": "4.9.5",
    "ua-parser-js": "^1.0.33",
    "usehooks-ts": "^2.9.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@svgr/webpack": "^6.5.1",
    "@types/gtag.js": "^0.0.12",
    "@types/is-touch-device": "^1.0.0",
    "@types/remove-markdown": "^0.3.1",
    "@types/ua-parser-js": "^0.7.36",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "eslint": "8.35.0",
    "eslint-config-next": "13.2.3",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.2",
    "next-sitemap": "^4.0.2",
    "prettier": "^2.8.4",
    "prettier-plugin-md-nocjsp": "^1.5.1",
    "sass": "^1.58.3",
    "stylelint": "^15.2.0",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-prettier-scss": "^0.0.1",
    "stylelint-config-recess-order": "^4.0.0",
    "stylelint-config-standard-scss": "^7.0.1"
  }
}
